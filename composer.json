{
  "name": "laravel-jp-reference/chapter8",
  "description": "Laravelリファレンス 8章実践Laravel",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Yuuki Takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.2.*",
    "predis/predis": "~1.0",
    "gregwar/captcha": "~1.0",
    "ytake/laravel-aspect": "~1.0",
    "laravel-doctrine/orm": "1.1.*",
    "laravelcollective/annotations": "5.2.*"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0",
    "doctrine/dbal": "~2.3",
    "barryvdh/laravel-ide-helper": "2.*",
    "laravel/homestead": "~3.0",
    "satooshi/php-coveralls": "*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helper.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "php env_check.php",
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php env_check.php",
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
